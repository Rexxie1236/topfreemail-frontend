{
"compilerOptions": {
"target": "ES2022",
"lib": ["DOM", "ES2022"],
"jsx": "react-jsx",
"module": "ESNext",
"moduleResolution": "Node",
"resolveJsonModule": true,
"esModuleInterop": true,
"skipLibCheck": true,
"forceConsistentCasingInFileNames": true,
"strict": true,
"noImplicitAny": false,
"isolatedModules": true,
"noEmit": true
},
"include": ["src"]
}
